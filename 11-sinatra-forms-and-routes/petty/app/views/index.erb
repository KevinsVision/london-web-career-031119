<h1>Pets Index</h1>

<ul>
  <% @pets.each do |pet| %>
    <li>
      <%= pet.name %>
      <form action="/pets/<%= pet.id%>" method="post">
        <input type="hidden", name="_method" value="delete"/>
        <div>
          <input type="submit" value="DESTROY!">
        </div>
      </form>
  </li>
  <% end %>
</ul>

